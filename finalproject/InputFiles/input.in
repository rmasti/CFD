f_case: 1;           // 1 for curvelinear MMS, 2 for 30 degree inlet, 3 for NACA

f_mesh: 1;           // 1-4 for all cases except the curvilinear which has 6

f_supersonic: 1;     // 1 for supersonic, 0 for subsonic

f_AOA: 2;            // 1 for 0 deg, 2 for 8 deg for case 3 only

f_upwind: 1;         // 1 for van leer, 2 for good ole Roe FDS

f_limiter: 0;        // 0 for limiter OFF, 1 for van albaada, 2 for ....

f_eps: 0;            // 1 for 2nd order, 0 for 1st order

f_kap: -1.0;            // CFL number for time stepping

rk_order: 2;         // specify runge kutta order (2, 4)

nmax: 1e6;           // max iteration number

wint: 100;           // write interval 

pint: 10;            // print interval (resid)

num_ghost: 3;        // number of ghost layers added

localdt: 0;          // 0 - use global dt, 1 - use local dt

tol: 1.0e-12;        // iterative tolerance for the solution

cfl: 0.1;            // CFL number for time stepping


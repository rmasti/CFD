f_case: 2;           // 1 for curvelinear MMS, 2 for 30 degree inlet, 3 for NACA

f_mesh: 1;           // 1-4 for all cases except the curvilinear which has 6

f_supersonic: 0;     // 1 for supersonic, 0 for subsonic

f_AOA: 1;            // 1 for 0 deg, 2 for 8 deg for case 3 only

f_upwind: 1;         // 1 for van leer, 2 for good ole Roe FDS

f_limiter: 2;        // 0 for limiter OFF, 1 for van leer, 2 for Van albaada, 3 ospre, 4 MCD, 5 MinMOD, 6 superbee....

f_eps: 1.0;            // 1 for 2nd order, 0 for 1st order

f_kap:  -1.0;         // kappa for determing upwind

rk_order: 4;         // specify runge kutta order (2, 4)

nmax: 1e6;           // max iteration number

wint: 1e4;           // write interval 

pint: 100;            // print interval (resid)

num_ghost: 3;        // number of ghost layers added

localdt: 0;          // 0 - use global dt, 1 - use local dt

tol: 1.0e-10;        // iterative tolerance for the solution

cfl: 0.8;            // CFL number for time stepping
